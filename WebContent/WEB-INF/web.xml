<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>wg_jsp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>member</servlet-name>
		<servlet-class>com.example.app.me.MemberFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>member</servlet-name>
		<url-pattern>*.me</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>matematch</servlet-name>
		<servlet-class>com.example.app.ma.MateMatchFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>matematch</servlet-name>
		<url-pattern>*.ma</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>freeboard</servlet-name>
		<servlet-class>com.example.app.fr.FreeboardFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>freeboard</servlet-name>
		<url-pattern>*.fr</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>qna</servlet-name>
		<servlet-class>com.example.app.qna.QnaFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>qna</servlet-name>
		<url-pattern>*.qna</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>notice</servlet-name>
		<servlet-class>com.example.app.no.NoticeFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>notice</servlet-name>
		<url-pattern>*.no</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>admin</servlet-name>
		<servlet-class>com.example.app.ad.AdminFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>admin</servlet-name>
		<url-pattern>*.ad</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>mypage</servlet-name>
		<servlet-class>com.example.app.my.MypageFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>mypage</servlet-name>
		<url-pattern>*.my</url-pattern>
	</servlet-mapping>
</web-app>

